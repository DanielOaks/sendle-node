{
  "name": "sendle-node",
  "sideEffects": false,
  "description": "Node.js library for the Sendle API",
  "version": "2.0.3",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": "https://github.com/Gomah/sendle-node",
  "homepage": "https://sendle-node.vercel.app",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "sendle",
    "sendle-node",
    "api",
    "library",
    "client"
  ],
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "prettier": {
    "printWidth": 100,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "scripts": {
    "start": "dts watch",
    "build": "dts build --target node",
    "test": "dts test --env=node",
    "lint": "dts lint",
    "prepare": "husky install",
    "prepublishOnly": "yarn build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "release": "yarn test && standard-version && git push --follow-tags && npm publish"
  },
  "author": "Gomah",
  "module": "dist/sendle-node.esm.js",
  "size-limit": [
    {
      "path": "dist/sendle-node.cjs.production.min.js",
      "limit": "40 KB",
      "ignore": [
        "fs",
        "path",
        "crypto",
        "buffer",
        "http",
        "https",
        "events",
        "stream",
        "dns",
        "http2",
        "tls",
        "net",
        "url",
        "os",
        "util",
        "zlib",
        "worker_threads"
      ]
    },
    {
      "path": "dist/sendle-node.esm.js",
      "limit": "40 KB",
      "ignore": [
        "fs",
        "path",
        "buffer",
        "crypto",
        "buffer",
        "http",
        "https",
        "events",
        "stream",
        "dns",
        "http2",
        "tls",
        "net",
        "url",
        "os",
        "util",
        "zlib",
        "worker_threads"
      ]
    }
  ],
  "devDependencies": {
    "@commitlint/cli": "^16.0.2",
    "@commitlint/config-conventional": "^16.2.1",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@tsconfig/recommended": "^1.0.1",
    "@types/node": "^17.0.23",
    "dotenv": "^16.0.0",
    "dts-cli": "^1.4.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "prettier": "^2.6.1",
    "size-limit": "^7.0.8",
    "standard-version": "^9.3.2",
    "ts-jest": "^27.1.4",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "got": "11.8.3",
    "hasha": "5.2.2",
    "hyperid": "3.0.1"
  }
}
