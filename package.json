{
  "name": "sendle-node",
  "description": "Node.js library for the Sendle API",
  "version": "1.0.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": "https://github.com/Gomah/sendle-node",
  "homepage": "https://sendle-node.vercel.app",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "sendle",
    "sendle-node",
    "api",
    "library",
    "client"
  ],
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "prettier": {
    "printWidth": 100,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build --target node",
    "test": "tsdx test --env=node",
    "lint": "eslint --ext .ts src/",
    "lint:tsdx": "tsdx lint",
    "prepare": "tsdx build --target node",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "release": "yarn test && standard-version && git push --follow-tags && npm publish"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "yarn lint"
    }
  },
  "author": "Gomah",
  "module": "dist/sendle-node.esm.js",
  "size-limit": [
    {
      "path": "dist/sendle-node.cjs.production.min.js",
      "limit": "100 KB",
      "ignore": [
        "fs",
        "path",
        "crypto",
        "dns",
        "http2",
        "tls",
        "net",
        "worker_threads"
      ]
    },
    {
      "path": "dist/sendle-node.esm.js",
      "limit": "100 KB",
      "ignore": [
        "fs",
        "path",
        "crypto",
        "dns",
        "http2",
        "tls",
        "net",
        "worker_threads"
      ]
    }
  ],
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@size-limit/preset-small-lib": "^5.0.1",
    "dotenv": "^10.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.0",
    "prettier": "^2.3.2",
    "size-limit": "^5.0.1",
    "standard-version": "^9.3.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "got": "11.8.2",
    "hasha": "5.2.2",
    "nanoid": "3.1.23"
  }
}
